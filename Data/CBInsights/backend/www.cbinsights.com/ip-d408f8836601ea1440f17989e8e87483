    <script type="text/javascript" src="./js/topsearch.min.js"></script>
    <script type="text/javascript" src="./js/cbi.js"></script>
    <script type="text/javascript" src="./js/timeline.js"></script>
    <script type="text/javascript" src="./js/tooltip.js"></script>
    <script type="text/javascript" src="./js/timeline-chart.js"></script>
    <script type="text/javascript">
     var myTimeline = Timeline();
     var myTimelineChart = TimelineChart().callback(function () {
	 $(".d3-loading-wrapper").removeClass("d3-loading-wrapper");
	 $(".d3-loading").remove();
     });
     var timeFormat = d3.time.format("%Y-%m-%d %X");
     var timeDisplayFormat = d3.time.format("%Y-%m");
     var i = 0;
     var data = [];
     d3.json('./ajax/getters/get_inv_people_news_mentions.php?id_inv_person_md5=d408f8836601ea1440f17989e8e87483', function (_data) {
	 if (_data.length >= 10) $('.white').slideDown();
	 if (_data.length == 0) {
	     $('[data-section=timeline]').hide();
	     $('.timeline').hide();
	 }
	 _data.reverse();
	 _data.forEach(function(e) {
	     e.ts_news = timeFormat.parse(e.ts_news);
	     if (data.length === 0) {
		 return data.push({ month: d3.time.month(e.ts_news), articles: [e] });
	     }
	     else {
		 if (timeDisplayFormat(e.ts_news) === timeDisplayFormat(data[i].month)) {
		     return data[i].articles.push(e);
		 }
		 else {
		     data.push({
			 month: d3.time.month(e.ts_news),
			 articles: [e]
		     });
		     return ++i;
		 }
	     }
	 });
	 d3.select("#nb-press-mentions").html(_data.length);
	 d3.select("#articles").datum(data).call(myTimeline);
	 d3.select("#chart").datum(data).call(myTimelineChart);
     });
    </script>
    
    <script type="text/javascript">
     $(document).ready(function () {
	 $('#more_bio,#less_bio').on('click', function() {
	     $('#bio_short').toggle();
	     $('#bio_long').toggle();
	 });//on click

	 $('.learn_more_twitter').on('click', function () {
	     $(this).parent().parent().children('.learn_more_twitter_div').slideToggle();
	     return false;
	 });
     });

     var active_page = 0;
     var rec_per_page = 5;
     function changepage(section, pagen, max_pagenum) {
	 if (pagen == "next")
	 active_page = Math.min(max_pagenum, active_page + 1);
	 else if (pagen == "previous")
	 active_page = Math.max(0, active_page -1);
	 else
	 active_page = pagen;

	 if (active_page < 5)
	 {
	     min_active_button = 0;
	     max_active_button = Math.min(max_pagenum, 9);
	 }
	 else if ((max_pagenum - active_page) < 5)
	 {
	     max_active_button = max_pagenum;
	     min_active_button = Math.max(0, max_pagenum - 9);
	 }
	 else
	 {
	     min_active_button = active_page - 4;
	     max_active_button = active_page + 5;
	 }

	 for (i=0; i <= max_pagenum; i++)
	 {
	     if (i == active_page) {
		 $('#' + section + String(i)).toggle(true);
		 $('#' + section + 'button' + String(i)).attr("class","active");
	     }
	     else {
		 $('#' + section + String(i)).toggle(false);
		 $('#' + section + 'button' + String(i)).attr("class","inactive");
	     }


	     if (i >= min_active_button && i <= max_active_button)
	     $('#' + section + 'button' + String(i)).toggle(true);
	     else
	     $('#' + section + 'button' + String(i)).toggle(false);
	 }
     }//changepage


     $('a[data-section]').on('click', function () {
	 $('.nav-pills li').removeClass('active');
	 $(this).parent().addClass('active');
	 if ($(this).attr('data-section-type') == 'core') {
	     $('#mosaic_tab').hide();
	     $('#news_tab').hide();
	     $('#basic_tab').show();
	     if ($(this).attr('data-section') == '') {
		 $('.section').show();
	     }
	     else {
		 $('.section').hide();
		 $('.section.' + $(this).attr('data-section')).show();
	     }

	     if ($('#total_score').css('display') != 'none') {
		 $('#company_header .span12').removeClass('span12').addClass('span9');
		 $('#top_right').removeClass('span9').addClass('span3').removeAttr('style');
		 $('#lists_menu').removeAttr('style');
		 $('#top_right > a').show();
		 $('#total_score').hide();
	     }
	 }
	 else {
	     $('#mosaic_tab').show();
	     $('#news_tab').hide();
	     $('#basic_tab').hide();

	     $('#mosaic_tab').html('<div id="mosaic-loader"><img src="img/ajax-loader.gif" alt="loading" /></div>');

	     var _this = $(this);
	     $.ajax({
		 type: 'POST',
		 url: 'ajax/getters/mosaic/' + _this.attr('data-section') + '.php',
		 data: {id_company: id_company},
		 success: function(data){
		     $('#mosaic_tab').html(data);
		     return false;
		 }
	     });
	     if ($('#total_score').css('display') == 'none') {
		 $('#top_right').removeClass('span3').addClass('span9').css('text-align', 'left');
		 $('#total_score').fadeIn();
		 $('#lists_menu').css({
		     'float': 'none',
		     'margin': 0,
		     'display': 'inline-block'
		 });
		 $('#top_right > a').hide();
	     }
	 }
     });

     $('#news_tab_button').on('click', function () {
	 $('#basic_tab').show();
	 $('#news_tab').show();
	 $('.pills_sub_menu li').removeClass('active');
     });

     $('a[data-section-type="core"]').each(function () { // hide tab if section is not present on the page
							if ($(this).attr('data-section') != '' && $('.section.' + $(this).attr('data-section')).length == 0)
							$(this).parent().hide();
							});
    </script>

